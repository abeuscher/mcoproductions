extends inc/page.pug

block page_id
  - var ptitle = "Booking"

block content
  :php
    foreach($page_data['text_blocks'] as $key => $block) {
      switch ($block["title"]) {
        case "Booking DJ Page":
          $content = $block;
          break;
        default:
          break;
      }
    }
  .container-fluid.content
    .row
      .col-xs-12
        h1 Book A DJ
        .user-content
          :php
            echo $content["Content"];
    .row.booking
      .col-xs-12
        include ./inc/dj-form.pug
