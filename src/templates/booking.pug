extends inc/page.pug

block page_id
  - var ptitle = "Booking"

block content
  :php
    foreach($page_data['text_blocks'] as $key => $block) {
      switch ($block["title"]) {
        case "Booking DJ Page":
          $content = $block;
          break;
        default:
          break;
      }
    }
  .row.content
    .col-12-xs
      h1 Book A DJ
      .user-content
        :php
          echo $content["Content"];
  .row.booking.content
    .col-12-xs  
      include ./inc/dj-form.pug
