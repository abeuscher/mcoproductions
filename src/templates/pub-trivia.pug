extends inc/page.pug

block page_id
  - var ptitle = "Pub Trivia"

block content
  :php
    foreach($page_data['text_blocks'] as $key => $block) {
      switch ($block["title"]) {
        case "Pub Trivia Page":
          $content = $block;
          break;
        default:
          break;
      }
    }
  .container-fluid.content
    .row
      .col-md-8
        .user-content
          :php
            echo $content["Content"];
        .booking
          h1 Book Trivia
          include ./inc/booking-form.pug          
      .col-md-4
        #triviaWidget.block.trivia

